{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.+(js|jsx|json|yml|yaml|css|vue)\"",
    "format:fix": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|vue)\""
  },
  "dependencies": {
    "@data-driven-forms/mui-component-mapper": "^2.24.0",
    "@data-driven-forms/react-form-renderer": "^2.24.0",
    "@emotion/cache": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@octokit/core": "^3.2.5",
    "aws-sdk": "^2.885.0",
    "next": "^10.1.3",
    "octokit-plugin-create-pull-request": "^3.9.3",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-loading-overlay": "^1.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
